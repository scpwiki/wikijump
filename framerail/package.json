    "eslint-plugin-tsdoc": "^0.2.17",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2",
